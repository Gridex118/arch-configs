#!/usr/bin/bash

systemctl --user import-environment WAYLAND_DISPLAY XDG_CURRENT_DESKTOP &
dbus-update-activation-environment --systemd DISPLAY WAYLAND_DISPLAY XDG_CURRENT_DESKTOP --all &

export GRIM_DEFAULT_DIR=~/Screenshots

# variables
scripts=~/.scripts

hyprpaper&
hypridle&
waybar&
eww daemon
eww open sidebar
